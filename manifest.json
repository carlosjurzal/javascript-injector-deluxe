{
  "manifest_version": 3,
  "name": "Javascript injector deluxe",
  "description": "This extension allows the user to execute JavaScript scripts on the current page.",
  "version": "1.0",
  "permissions": ["activeTab", "tabs", "storage"],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  }
}